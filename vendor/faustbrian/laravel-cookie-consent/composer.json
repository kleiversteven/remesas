{
    "name": "faustbrian/laravel-cookie-consent",
    "description": "Easy drop-in Cookie Consent (Silktide) for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Cookie-Consent", "Laravel CookieConsent", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.4.* || 5.5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\CookieConsent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\CookieConsent\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\CookieConsent\\CookieConsentServiceProvider"
            ],
            "aliases": {
                "CookieConsent": "BrianFaust\\CookieConsent\\Facades\\CookieConsent"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
